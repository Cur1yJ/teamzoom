$("#edit_details").modal('hide');
$("#find_or_initialize_teamsport").html("#{j(render("admin/teams/sports_team_edit", :teamsport => @teamsport))}");
$("#edit_details").modal('show');
$("#venue-tab a:last").tab('show');
-if @teamsport.errors.any?
